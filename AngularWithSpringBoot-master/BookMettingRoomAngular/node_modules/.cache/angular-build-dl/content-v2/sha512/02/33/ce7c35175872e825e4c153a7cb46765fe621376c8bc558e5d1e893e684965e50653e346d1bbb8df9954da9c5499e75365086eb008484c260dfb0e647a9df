() {\n    return this.http.get(\"http://localhost:8080/api/auth/dept\");\n  }\n\n  datetime(info: DateTime): Observable<any> {\n    return this.http.post<any>(\"http://localhost:8080/api/bookRoomForm/Datetime\", info, httpOptions);\n  }\n  \n  loggedIn() {\n    return sessionStorage.getItem('AuthToken');\n  }\n}","export class DateTime {\n\n    constructor(\n        public date1: any,\n        public date2: any,\n    ) { }\n}","export class AuthLoginInfo {\n    email: string;\n    password: string;\n  \n    constructor(email: string, password: string) {\n        this.email = email;\n        this.password = password;\n    }\n  }","export class Mails {\n    constructor(\n        private user_mail:string,\n        private newEmail:string,\n        private roles: any,\n        private department: string,\n    ) { }\n\n}","export class Room {\n\n    constructor(\n        public id: number,\n        public name: string,\n        public location: string,\n        public facility: string,\n    ) { }\n}","import { Department } from './dept';\n\nexport class SignUpInfo {\n    name: string;\n    username: string;\n    email: string;\n    role: string[];\n    password: string;\n    department: string;\n \n    constructor(name: string, username: string, email: string, password: string, department: string) {\n        this.name = name;\n        this.username = username;\n        this.email = email;\n        this.password = password;\n        this.role = ['user'];\n        this.department = department;\n    }\n}","import { Injectable } from '@angular/core';\n \nconst TOKEN_KEY = 'AuthToken';\nconst USERNAME_KEY = 'AuthUsername';\nconst AUTHORITIES_KEY = 'AuthAuthorities';\nconst EMAIL_KEY =  'AuthEmail';\n\n@Injectable({\n  providedIn: 'root'\n})\nexport class TokenStorageService {\n  private roles: Array<string> = [];\n  constructor() { }\n \n  signOut() {\n    window.sessionStorage.removeItem(USERNAME_KEY);\n    window.sessionStorage.removeItem(EMAIL_KEY);\n    window.sessionStor